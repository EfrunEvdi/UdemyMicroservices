{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:33956",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "FreeCourse.Services.Catalog": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true, // bir komutun veya yapılandırma ayarının bir web tarayıcısını başlatmamasını belirten bir ifadedir. Özellikle bir yazılımda, betikte veya kod parçasında bu tür bir ayar, belirli bir işlemin veya görevlerin gerçekleştirilirken tarayıcı penceresinin açılmamasını sağlar. Bu, örneğin, testlerin otomatik olarak çalıştırıldığı senaryolarda veya tarayıcı etkileşiminin istenmediği durumlarda kullanılabilir.
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5011",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
